<div class="">
	<div class="row">

		{{> adminNav  }}

		<div role="main"  class="col-sm-9 col-md-10 main">
			<table class="table table-striped table-hover">
				<thead>
					<tr>
						<td>Nr</td>
						<td>Name</td>
						<td>Status</td>
						<td>Betrieb</td>
						<td>Ziel Punkte</td>
						<td>Gültig ab</td>
						{{!-- <td>Lesbar bis</td> --}}
						<td>Einlösbar bis</td>
						<td>Rollen</td>
					</tr>
				</thead>
				<tbody>
					{{#programs}}
					<tr class='clickableRow' onclick="window.document.location='/programm/{{nr}}';"> 
						<td>{{nr}}</td>
						<td>{{programName}}</td>
						<td>{{programStatus}}</td>
						<td>{{company}}</td>
						<td>{{goalToHit}}</td>
						<td>{{startDate}}</td>
						{{!-- <td>{{deadlineScan}}</td> --}}
						<td>{{deadlineSubmit}}</td>
						<td>{{#allocatedReels}}
							{{#if @index}}, {{/if}}<a href="/rollen/{{nr}}">{{nr}}</a>{{/allocatedReels}}</td>
					</tr>
					{{/programs}}
				</tbody>
			</table>
		</div>

	</div>
</div>