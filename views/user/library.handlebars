<h4>{{customerNr}} {{customerCompany}} zugeordnete User</h4></br>
<a href="/user/anlegen" class="btn btn-default">Weiteren User anlegen</a>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<td>Name</td>
			<td>Username</td>
			<td>Rolle</td>
			<td>Seit</td>
		</tr>
	</thead>
	<tbody>
		{{#users}}
		<tr class='clickableRow'> <!-- onclick="window.document.location='/programm/{{nr}}';" --> 
			<td>{{firstName}} {{lastName}}</td>
			<td>{{username}}</td>
			<td>{{role}}</td>
			<td>{{created}}</td>
		</tr>
		{{/users}}
	</tbody>
	
</table>