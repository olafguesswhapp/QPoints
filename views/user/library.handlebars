<h3>Kunden {{customerNr}} {{customerFirma}} zugeordnete User</h3>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<td>Name</td>
			<td>Username</td>
			<td>Rolle</td>
			<td>Seit</td>
		</tr>
	</thead>
	<tbody>
		{{#users}}
		<tr class='clickableRow'> <!-- onclick="window.document.location='/programm/{{nr}}';" --> 
			<td>{{name}}</td>
			<td>{{username}}</td>
			<td>{{role}}</td>
			<td>{{created}}</td>
		</tr>
		{{/users}}
	</tbody>
	
</table>